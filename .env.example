# X/Twitter Authentication (required)
# Extract from browser: F12 → Application → Cookies → x.com
AUTH_TOKEN=your_auth_token_here
CT0=your_ct0_token_here

# Proxy Configuration (optional)
# Supports SOCKS5 proxies for bypassing rate limits
PROXY_URL=socks5://user:pass@host:port

# Scraper Settings
PARALLEL_WORKERS=5
MAX_RETRY=5
RETRY_WAIT=10

# Logging
LOG_LEVEL=INFO
